from sklearn import linear_model

#accidents, jaywalkers, road debris
traffic_data = [
    [1, 9],
    [1, 10],
    [1, 11],
    [1, 12],
    [1, 13],
    [1, 14],
    [1, 15],
    [1, 16],
]

output_data = [0,10,20,30,40,30,20,10]

model = linear_model.LinearRegression()

model.fit(traffic_data, output_data)

print(model.predict([[1,15]]))

